<!DOCTYPE html>
<html lang="{{ with .Site.LanguageCode }}{{ . }}{{ else }}en-US{{ end }}">
	<head>
		{{ partial "head.html" . }}
	</head>
	<body>
    {{ define "main" }}
    <main class="products">
    {{ range .Site.Data.products }}
      {{ partial "header.html" . }}
      {{ partial "product" . }}
      {{ partial "footer.html"  . }}
    {{ end }}
    </main>
    {{ end }}
		

		<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
		<script src="{{ .Site.BaseURL }}js/functions.js"></script>

	</body>
</html>
